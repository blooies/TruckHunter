<div id='header'>
	<h1>TruckHunter</h1>
	<div id="searchbox">
		<form id="searchform">
			<input type="text" name="foodtruck"  placeholder="search for a foodtruck!" id="autocomplete">
		</form>
	</div>
</div>



<div id='map' class='dark'></div>
<a href='#' id='geolocate' class='ui-button'>Find me</a>



<div id="newtrucktab">
	<p><%= @message %></p>

	<div id="newtruckform">
		<div id="close">
			<text>&#10005;<text>
		</div>
		<%= form_for @newtruck, url: "/addtruck" do |f| %>
			Truck <%= f.label :name %><br>
			<%= f.text_field :name%><br>
			Truck's <%= f.label :twitter_handle %> 
			<%= f.text_field :twitter_handle %>
			<%= f.submit "let us know!"%>
		<% end %>
	</div>

</div>



<div id="truckownertab">
	<p>get your truck on the map</p>
</div>



<div id="readme">
	<div id="close">
		<text>&#10005;<text>
	</div>
	<p>yo</p>
</div>





